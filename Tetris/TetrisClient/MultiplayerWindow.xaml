<Window x:Class="TetrisClient.MultiplayerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:TetrisClient"
        mc:Ignorable="d"
        Title="MultiplayerWindow"  Height="550" Width="400">
    <Grid>
        <Button Content="Ready up!" Click="StartGame_OnClick" Margin="76,254,92,224" Background="#FF2F7FDE"></Button>
        <Label Name="Status" Content="Waiting for Connection..." HorizontalAlignment="Center" Margin="0,156,0,0" VerticalAlignment="Top" Height="25" Width="142"/>
        <Button Content="&#xE112; Go Back" Click="GoBackButton" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="27" Width="78" Background="{x:Null}" Cursor="Hand" FontFamily="Segoe UI Symbol" Foreground="#FF7C7C7C" BorderBrush="{x:Null}"/>
    </Grid>
</Window>
